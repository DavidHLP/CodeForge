{
  "properties": [
    {
      "name": "spring.data.redis.commons.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用 Redis Commons 功能",
      "defaultValue": true
    },
    {
      "name": "spring.data.redis.commons.cache.default-ttl",
      "type": "java.time.Duration",
      "description": "默认缓存过期时间",
      "defaultValue": "PT1H"
    },
    {
      "name": "spring.data.redis.commons.cache.key-prefix",
      "type": "java.lang.String",
      "description": "缓存键前缀",
      "defaultValue": "springoj:cache:"
    },
    {
      "name": "spring.data.redis.commons.cache.enable-null-values",
      "type": "java.lang.Boolean",
      "description": "是否启用空值缓存",
      "defaultValue": false
    },
    {
      "name": "spring.data.redis.commons.cache.serialization-type",
      "type": "java.lang.String",
      "description": "缓存序列化类型",
      "defaultValue": "JSON"
    },
    {
      "name": "spring.data.redis.commons.lock.default-wait-time",
      "type": "java.time.Duration",
      "description": "分布式锁默认等待时间",
      "defaultValue": "PT10S"
    },
    {
      "name": "spring.data.redis.commons.lock.default-lease-time",
      "type": "java.time.Duration",
      "description": "分布式锁默认租约时间",
      "defaultValue": "PT30S"
    },
    {
      "name": "spring.data.redis.commons.lock.retry-attempts",
      "type": "java.lang.Integer",
      "description": "分布式锁重试次数",
      "defaultValue": 3
    },
    {
      "name": "spring.data.redis.commons.lock.key-prefix",
      "type": "java.lang.String",
      "description": "锁键前缀",
      "defaultValue": "springoj:lock:"
    },
    {
      "name": "spring.data.redis.commons.transaction.enable-auto-rollback",
      "type": "java.lang.Boolean",
      "description": "是否启用事务自动回滚",
      "defaultValue": true
    },
    {
      "name": "spring.data.redis.commons.transaction.timeout",
      "type": "java.time.Duration",
      "description": "事务超时时间",
      "defaultValue": "PT30S"
    },
    {
      "name": "spring.data.redis.commons.transaction.enable-transaction-log",
      "type": "java.lang.Boolean",
      "description": "是否启用事务日志",
      "defaultValue": false
    },
    {
      "name": "spring.data.redis.commons.metrics.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用监控指标",
      "defaultValue": true
    },
    {
      "name": "spring.data.redis.commons.metrics.slow-query-threshold",
      "type": "java.time.Duration",
      "description": "慢查询阈值",
      "defaultValue": "PT0.1S"
    },
    {
      "name": "spring.data.redis.commons.metrics.enable-detailed-metrics",
      "type": "java.lang.Boolean",
      "description": "是否启用详细指标",
      "defaultValue": false
    },
    {
      "name": "spring.data.redis.commons.metrics.collection-interval",
      "type": "java.time.Duration",
      "description": "指标收集间隔",
      "defaultValue": "PT30S"
    }
  ]
}